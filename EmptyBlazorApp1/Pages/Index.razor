@page "/"
@using EmptyBlazorApp1.Entities

@if (User is not null) {
    <Logined User="@User"/>
}
else {
    <LoginComponent/>
}

@{
    init();
}

@code {

    [Parameter]
    public string? Param { get; set; } = string.Empty;

    public User? User { get; set; }

    void init() {
        Console.WriteLine(Param);
    }

}